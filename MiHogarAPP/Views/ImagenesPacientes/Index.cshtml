@model IEnumerable<ConexionMiHogarAPP.Models.ImagenesPacientes>

@{
    ViewBag.Title = "Index";
}

    <h2>Inicio Imagenes Pacientes </h2>

    <p>
        @Html.ActionLink("Ingresar Nueva Imagen Paciente", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pacientes.rut)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @*@Html.DisplayFor(modelItem => item.url)*@
                    <img width="100" src="@item.url" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pacientes.rut)
                </td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Detalles", "Details", new { id = item.id }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.id })
                </td>
            </tr>
        }

    </table>
